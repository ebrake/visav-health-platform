{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "include" :["client/src/components", "client/src/alt", "client/scripts"],
        "includePattern": ".+\\.js(jsx)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "includeDate": false
        }
    },
    "opts": {
        "encoding": "utf8",
        "recurse" : true,
        "destination": "./documentation/",
        "template": "node_modules/docdash"
    }
}

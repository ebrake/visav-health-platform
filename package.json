{
  "name": "dillinger",
  "version": "1.0.0",
  "scripts": {
    "build-prod": "npm run clean-prod && cp -rf server server-dist && ./node_modules/.bin/babel --optional runtime -d server-dist ./server",
    "start-prod": "node ./server-dist/server.init-prod",
    "clean-prod": "rm -rf server-dist || true",
    "postinstall": "npm run clean-prod && npm run build-prod",
    "pretest": "eslint .",
    "posttest": "nsp check"
  },
  "dependencies": {
    "babel-cli": "^6.11.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-node6": "^11.0.0",

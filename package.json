    "compression": "^1.0.3",
    "consolidate": "^0.14.1",
    "cors": "^2.5.2",
    "fs": "0.0.2",
    "helmet": "^1.3.0",
    "loopback": "^3.0.0-alpha.1",
    "loopback-boot": "^2.6.5",
    "loopback-component-explorer": "^2.4.0",
    "loopback-component-push": "^1.5.4",
    "loopback-connector-postgresql": "^2.6.3",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-router": "^2.6.0",
    "serve-favicon": "^2.0.1",
    "strong-error-handler": "^1.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.11.4",

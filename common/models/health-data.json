{
  "name": "HealthData",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "type": {
      "type": "string",
      "required": true,
      "default": "exercise"
    },
    "data": {
      "type": "object",
      "required": true,
      "default": {}
    },
    "description": {
      "type": "string",
      "default": ""
    },
    "createdDate": {
      "type": "date",
      "required": true
    },
    "startDate": {
      "type": "date",
      "required": true
    },
    "endDate": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "Person": {
      "type": "belongsTo",
      "model": "Person",
      "foreignKey": "person"
    }
  },
  "indexes": {
    "unique_person_and_date": {
      "keys": {
         "person": 1,
         "startDate": -1
       },
       "options": {
         "unique": true
       }
    }
  },
  "acls": [],
  "methods": {}
}

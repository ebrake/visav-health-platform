{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "include" :["server"],
        "includePattern": ".+\\.js(jsx)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default":{
            "useLongnameInNav": true
        }
    },
    "opts": {
        "recurse" : true, 
        "destination": "./QA/jsdoc/server" 
    }
}
